{
  "editor.fontFamily": "JetBrainsMono Nerd Font, JetBrains Mono, SF Mono, Input Mono, monospace",
  "editor.fontWeight": "400",
  "editor.fontSize": 14,
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss06', 'zero'",
  "editor.guides.bracketPairs": "active",
  "window.autoDetectColorScheme": true,
  "workbench.editor.tabActionLocation": "left",
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.expandMode": "singleClick",
  "workbench.tree.indent": 10,
  "debug.onTaskErrors": "debugAnyway",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.inlineSuggest.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.stickyScroll.enabled": true,
  "editor.hover.sticky": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "scm.diffDecorationsGutterWidth": 2,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.tabs.enabled": true,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  // "workbench.editor.limit.enabled": true,
  // "workbench.editor.limit.perEditorGroup": true,
  "search.exclude": {
      "**/bundlers/static": true,
      "**/tests/": true,
      "**/*.snap": true,
      "**/*.svg": true,
      "**/.git": true,
      "**/.github": false,
      "**/.nuxt": true,
      "**/.output": true,
      "**/.pnpm": true,
      "**/.vscode": true,
      "**/.yarn": true,
      "**/bower_components": true,
      "**/dist/**": true,
      "**/logs": true,
      "**/node_modules": true,
      "**/out/**": true,
      "**/package-lock.json": true,
      "**/pnpm-lock.yaml": true,
      "**/temp": true,
      "**/yarn.lock": true,
      "**/CHANGELOG*": true,
      "**/LICENSE*": true,
      "**/build/**": true,
      "**/tmp": true,
      "**/build/static/**": true,
      "**/lib/cjs/**": true,
      "**/lib/esm/**": true
  },
  "window.dialogStyle": "custom",
  "window.title": "${rootName}",
  "window.titleBarStyle": "custom",
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": false,
  "errorLens.enabledDiagnosticLevels": [
      "warning",
      "error",
      "hint",
      "info"
  ],
  "errorLens.excludeBySource": [
      "cSpell",
      "Grammarly",
      "eslint"
  ],
  "eslint.codeAction.showDocumentation": {
      "enable": true
  },
  "eslint.quiet": false,
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "vue",
      "html",
      "markdown",
      "json",
      "jsonc",
      "yaml",
      "toml"
  ],
  "cSpell.allowCompoundWords": true,
  "cSpell.language": "en,en-US",
  "css.lint.hexColorLength": "ignore",
  "cSpell.ignorePaths": [],
  "cSpell.diagnosticLevel": "Hint",
  "prettier.enable": false,
  "prettier.printWidth": 200,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "explorer.fileNesting.enabled": false,
  "explorer.fileNesting.expand": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.exclude": {},
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorWidth": 3,
  "[jsonc]": {
      "editor.defaultFormatter": "vscode.json-language-features"
  },
  "typescript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "[typescript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
      "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[shellscript]": {
      "editor.defaultFormatter": "shakram02.bash-beautify"
  },
  "[go]": {
      "editor.defaultFormatter": "golang.go",
      "editor.insertSpaces": true
  },
  "[markdown]": {
      "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  // "search.collapseResults": "alwaysCollapse",
  "go.toolsManagement.autoUpdate": true,
  "[html]": {
      "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[typescriptreact]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[css]": {
      "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "[svelte]": {
      "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "editor.accessibilitySupport": "off",
  "editor.insertSpaces": true,
  "extensions.ignoreRecommendations": true,
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.defaultFormatter": null,
  "editor.cursorBlinking": "expand",
  "editor.fastScrollSensitivity": 5,
  "editor.cursorSmoothCaretAnimation": "off",
  "editor.smoothScrolling": false,
  "editor.lineNumbers": "relative",
  "zenMode.hideLineNumbers": false,
  "vim.smartRelativeLine": false,
  "vim.useSystemClipboard": true,
  "vim.foldfix": true,
  "vim.easymotion": false,
  "vim.incsearch": false,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": false,
  "vim.handleKeys": {
      "<C-a>": true,
      "<C-f>": true
  },
  "[scss]": {
      "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.codeLens": true,
  "vsicons.dontShowNewVersionMessage": true,
  // "workbench.editor.enablePreview": false,
  "chat.commandCenter.enabled": false,
  "cSpell.enabled": true,
  "files.watcherExclude": {
      "**/.git": true,
      "**/.gitignore": false,
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/.hg/store/**": true,
      "**/.svn": false,
      "**/*.js.map": true,
      "**/cjs/*": true,
      "**/esm/*": true,
      "**/npm-lock.*": true,
      "**/pnpm-lock.*": true,
      "**/target/*": true,
      "**/build/*": true,
      "**/*.snap": true,
      "**/.nuxt": true,
      "**/.output": true,
      "**/.pnpm": true,
      "**/.yarn": true,
      "**/bower_components": true,
      "**/dist/**": true,
      "**/logs": true,
      "**/node_modules": true,
      "**/out/**": true,
      "**/package-lock.json": true,
      "**/pnpm-lock.yaml": true,
      "**/temp": true,
      "**/yarn.lock": true,
      "**/CHANGELOG*": true,
      "**/LICENSE*": true,
      "**/build/**": true,
      "**/tmp": true,
      "**/build/static/**": true
  },
  "workbench.settings.enableNaturalLanguageSearch": false,
  "explorer.confirmPasteNative": false,
  "search.followSymlinks": false,
  "[vue]": {
      "editor.defaultFormatter": "Vue.volar"
  },
  "[javascript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.renderWhitespace": "boundary",
  "workbench.fontAliasing": "antialiased",
  "workbench.sideBar.location": "right",
  "intelephense.environment.phpVersion": "8.3.6",
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "github.copilot.enable": {
      "*": true,
      "plaintext": false,
      "markdown": true,
      "scminput": false
  },
  "window.confirmSaveUntitledWorkspace": false,
  "svelte.enable-ts-plugin": true,
  "githubPullRequests.pullBranch": "never",
  "security.promptForLocalFileProtocolHandling": false,
  "workbench.iconTheme": "vscode-icons",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.preferredDarkColorTheme": "Gruvbox Dark Hard",
  "workbench.preferredLightColorTheme": "Gruvbox Light Hard",
  "cursor.cpp.disabledLanguages": [
      "plaintext",
      "scminput"
  ],
  "workbench.editor.wrapTabs": true,
  "workbench.activityBar.location": "hidden"
}
